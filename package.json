{
  "name": "app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules  ./src/server.ts",
    "commit": "git add .& cz",
    "typeorm": "ts-node-dev node_modules/typeorm/cli.js",
    "test": "NODE_ENV=test jest",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/database/index.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/database/index.ts",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/database/migrations/UpdateSchema -d ./src/database/index.ts",
    "migration:create": "typeorm migration:create ./src/database/migrations/UpdateSchema",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@types/bcrypt": "^3.0.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node-schedule": "^1.3.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/parser": "^8.13.0",
    "commitlint": "^19.5.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "pg": "^8.11.5",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/node": "^20.12.8",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "logger": "^0.0.1",
    "node-schedule": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.3.20",
    "uuid": "^8.3.2",
    "winston": "^3.16.0",
    "yarn": "^1.22.22"
  }
}
